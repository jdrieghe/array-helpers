{
    "name": "jdrieghe/array-helpers",
    "description": "A collection of convenience methods to deal with arrays in php. Somewhat inspired by Laravel array helpers.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Drieghe",
            "email": "jonasd@madewithlove.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpunit/php-code-coverage": "^6.0"
    },
    "autoload": {
        "files": [
            "src/array-helpers.php"
        ],
        "psr-4": {
            "ArrayHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArrayHelpers\\": "tests/"
        }
    }
}
